{
	"$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
	"files": {
		"ignore": ["**/*.typegen.ts"]
	},
	"linter": {
		"enabled": true,
		"rules": {
			"all": true,
			"nursery": {
				"all": false
			}
		}
	},
	"organizeImports": {
		"enabled": true
	},
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	}
}
